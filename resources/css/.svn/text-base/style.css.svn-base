a.menu-item {
    display: block;
    padding: 1em;
    font-weight: bold;
    text-decoration: none;
    border-radius: 10px;
    box-shadow: 1px 1px 4px #768395;
    //background: -webkit-linear-gradient(top, #fff, #ddd);
}

.search-panel {
    z-index: 10000;
    background: url(../images/search-panel-bg.png) repeat;
}

.secondary{
	display:block;
	font-size:10px !important;
	color:#d40e3f;
	margin-left:.5em;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis
}

.list-item-parent {
    background: url(../images/chevron-normal.png) no-repeat scroll right center;
    -webkit-background-origin: content-box;
    -webkit-background-size: 3ex 3ex;
}

.tdPad {
	padding:10px !important;
}
